{
  "name": "business-papers-redesign",
  "version": "1.1.0",
  "description": "Redesign of House of Commons business papers pages",
  "main": "index.html",
  "scripts": {
    "deploy": "gh-pages -d demo",
    "watch-sass": "sass --watch sass:demo/css --style=expanded",
    "liveserver": "live-server --port=80 --wait=400 demo/",
    "serve": "concurrently --kill-others \"npm:watch-sass\" \"npm:liveserver\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoc-ppu/2025-biz-paper-html.git"
  },
  "author": "Mark Fawcett",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/hoc-ppu/2025-biz-paper-html/issues"
  },
  "homepage": "https://github.com/hoc-ppu/2025-biz-paper-html#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.1",
    "sass": "^1.77.6"
  },
  "dependencies": {
    "@compodoc/live-server": "^1.2.2",
    "cors": "^2.8.5",
    "live-server": "^1.2.2"
  }
}
